    <bugTrackerUrl>https://github.com/tygerbytes/posh-dynargs/issues</bugTrackerUrl>
    <releaseNotes>https://github.com/tygerbytes/posh-dynargs/blob/v0.1.0.4/CHANGELOG.md</releaseNotes>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <dependencies>
      <dependency id="chocolatey" version="0.9.10" />
    </dependencies>
  </metadata>
